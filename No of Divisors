class Solution{
    static long count_divisors(int N){
        // code here
        long count=0;
       
       if(N%3==0)
               {
                 for(int i=3;i<=N;i++)
                 {
                     if(N%i==0 && i%3==0)
                     {  
                         count++;
                     }
                 }
                 return count;
                }
        return 0;
    }
}
