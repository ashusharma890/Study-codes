class Solution
{
    public long sum_of_square_evenNumbers(long n)
    {
        // Code here
        long square=0;
        long sum=0;
        long num=2;
        for(int i=0;i<n;i++)
        {
        square=(long)Math.pow(num,2);
        sum=sum+square;
        num+=2;
        }
        return sum;
    }
}
