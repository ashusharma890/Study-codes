class Solution
{
    public long[] find(int l, int b, int h)
    {
        // code here
          Long L= new Long(l);
          Long B= new Long(b);
          Long H= new Long(h);
        long surfaceArea= 2*(B*H+L*H+L*B);
        long volume= L*B*H;
        long[] arr= new long[2];
        arr[0]=surfaceArea;
        arr[1]=volume;
        
        return arr;
    }
}
